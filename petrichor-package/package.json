{
  "name": "retrosite",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/fab": "^14.0.0",
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node-sass": "^4.11.3",
    "node-sass": "^8.0.0",
    "sass": "^1.62.0",
    "smui-theme": "^7.0.0-beta.8",
    "svelte": "^3.54.0",
    "svelte-check": "^2.10.0",
    "svelte-preprocess": "^5.0.3",
    "svelte-tooltip": "^1.2.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "@smui-extra/bottom-app-bar": "^7.0.0-beta.8",
    "@smui/fab": "^7.0.0-beta.8",
    "fuzzysort": "^2.0.4",
    "github-markdown-css": "^5.2.0",
    "highlight.js": "^11.7.0",
    "svelte-highlight": "^7.2.1",
    "svelte-markdown": "^0.2.3",
    "svelte-material-ui": "^7.0.0-beta.8"
  }
}